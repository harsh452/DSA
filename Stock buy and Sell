int this question all the elements of the array are represented as days;

 int maxProfit(vector<int>& prices) {
        int max1 = INT_MAX;
        
        int op1 = 0;
        int dp1 = 0;
        
        for(int i=0;i<prices.size();i++){
            if(prices[i]<max1){
                max1 = prices[i];
            }
            
            dp1 = prices[i] - max1;
            if(op1<dp1){
                op1 = dp1;
            }
        }
        return op1;
